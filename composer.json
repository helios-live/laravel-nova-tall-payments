{
    "name": "alexeftimie/laravel-payments",
    "description": "Payments and Subscriptions implementation for Laravel",
    "type": "library",
    "authors": [
        {
            "name": "Alexandru Eftimie",
            "email": "office@alexeftimie.ro"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "AlexEftimie\\LaravelPayments\\LaravelPaymentsProvider"
            ],
            "aliases": {
                "Larapay": "AlexEftimie\\LaravelPayments\\Facades\\Larapay"
            }
        }
    },
    "require": {
        "appstract/laravel-meta": "^2.0",
        "stripe/stripe-php": "^7.67",
        "srmklive/paypal": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "AlexEftimie\\LaravelPayments\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AlexEftimie\\LaravelPayments\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "orchestra/testbench-dusk": "^6.7",
        "phpunit/phpunit": "^9.5"
    }
}
