{
    "name": "ideatocode/laravel-nova-tall-payments",
    "description": "Payments and Subscriptions implementation for Laravel",
    "type": "library",
    "authors": [
        {
            "name": "Alexandru Eftimie",
            "email": "office@ideatocode.tech"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "IdeaToCode\\LaravelNovaTallPayments\\ServiceProvider"
            ],
            "aliases": {
                "Larapay": "IdeaToCode\\LaravelNovaTallPayments\\Facades\\Larapay",
                "Product": "IdeaToCode\\LaravelNovaTallPayments\\Models\\Product"
            }
        }
    },
    "require": {
        "appstract/laravel-meta": "^2.0",
        "stripe/stripe-php": "^7.67",
        "srmklive/paypal": "^3.0",
        "shakurov/coinbase": "^0.7.7",
        "spatie/nova-tags-field": "3.4.1"
    },
    "autoload": {
        "psr-4": {
            "IdeaToCode\\LaravelNovaTallPayments\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IdeaToCode\\LaravelNovaTallPayments\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "orchestra/testbench-dusk": "^6.7",
        "phpunit/phpunit": "^9.5"
    }
}